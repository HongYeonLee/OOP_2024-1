package Week4;
import java.util.Scanner;

public class 과제3_트리만들기 {
	public static void main(String[] args)
	{
		int 가로, 세로;
		
		Scanner input = new Scanner(System.in);
		System.out.println("가로: ");
		가로 = input.nextInt();
		
		System.out.println("세로: ");
		세로 = input.nextInt();
		
		for (int i = 0; i <가로; i++)
		{
			for (int j = 0; j <세로; j++)
				System.out.print("*");
			System.out.println();
		}

	
		
		
		
		
		
	}

}
