package Week3;
import java.util.Scanner;
public class 학점매기기 {
	public static void main (String[] args)
	{
		int score;
		char grade;
		Scanner input = new Scanner(System.in);
		System.out.println("점수를 입력하세요 (0 ~ 100) : ");
		score = input.nextInt();
		
		if (score >= 90)
			grade = 'A';
		else if (score >= 80)
			grade = 'B';
		else if (score >= 70)
			grade = 'C';
		else if (score >= 60)
			grade = 'D';
		else
			grade = 'F';
		
		// + , - 붙이기
		int 일의자리 = score%10;
		
		input.close();
	}
}
